{
  "name": "@lskjs/cli",
  "version": "1.12.0",
  "author": "Igor Suvorov @isuvorov",
  "bin": {
    "lsk": "bin/run"
  },
  "bugs": "https://github.com/lskjs/cli/issues",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.13",
    "@lskjs/log2": "^2.2.0-beta.13",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "glob": "^7.1.6",
    "kleur": "^4.1.4",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@lskjs/eslint-config": "2.1.0",
    "@oclif/dev-cli": "^1",
    "eslint": "7.19.0",
    "globby": "^10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/utils"
  ],
  "homepage": "https://github.com/isuvorov/lsk",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "lsk",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "lskjs/cli",
  "scripts": {
    "release": "npm version minor && npm publish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "lint:js": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "version": "oclif-dev readme && git add README.md"
  }
}
