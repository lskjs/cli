{
  "name": "@lskjs/cli",
  "version": "1.18.1",
  "description": "LSK.js CLI (Command Line Interface) is the easiest way to create and manage LSK.js project",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "keywords": [
    "lskjs",
    "lsk",
    "cli",
    "helper"
  ],
  "license": "MIT",
  "bugs": "https://github.com/lskjs/cli/issues",
  "homepage": "https://github.com/lskjs/cli",
  "repository": "lskjs/cli",
  "bin": {
    "lsk": "bin/run",
    "lskjs": "bin/run"
  },
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "@lskjs/cli-scripts": "1.18.1",
    "@lskjs/cli-utils": "1.17.6",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^10"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/utils.js"
  ],
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "lsk",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "lint:js": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "version": "oclif-dev readme && git add README.md && rm -f ../../README.md && cp README.md ../.."
  }
}
